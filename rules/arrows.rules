arrow
← = <-
→ = ->
↔ = <-> <> ←> <→ ←→
↓ = v {| v}
↑ = ^ {| ^}
↖ = {\ ^}
↗ = {/ ^}
↘ = {\ v}
↙ = {/ v}
↕ = {^ v} {^ | v} {↑ ↓}

double arrow
⇐ = <= {← =}
⇒ = => {→ =}
⇑ = {= ^} {| | ^}
⇓ = {= v} {| | v}
⇔ = <=> ⇐> <⇒ {↔ =} ⇐⇒

crossed arrow
↚ = {← /} {<- /} </ </-
↛ = {→ /} {-> /} /> -/>
⇍ = {⇐ /} {<= /}
⇏ = {⇒ /} {=> /}

2-headed arrow
↠ = ->> >> →> →→
↞ = <<- << <← ←←
↟ = ↑↑ {↑ ^} {| ^ ^} {^ ^}
↡ = ↓↓ {↓ v} {| v v} {v v}

arrow with tail
↢ = ←< <-< <<
↣ = >→ >-> >>

arrow with bar
↤ = ←| <-| <|
↦ = |→ |-> |>
↥ = {↑ _} {^ | _} {^ _}
↧ = {↓ _} {v | _} {v _} {T v}

paired arrow
⇄ = →←
⇆ = ←→
⇉ = →→ =>> -->>
⇇ = ←← <<= <<--
⇅ = ↑↓ ^v
⇈ = ↑↑ ^^
⇊ = ↓↓ vv
⇵ = ↓↑ v^

paired arrow (backwards order)
⇄ = ←→
⇆ = →←
⇅ = ↓↑ v^
⇵ = ↑↓ ^v
