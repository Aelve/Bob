arrow
← = <-
→ = ->
↔ = <-> <> ←> <→
↓ = v {| v}
↑ = ^ {| ^}
↖ = {\ ^}
↗ = {/ ^}
↘ = {\ v}
↙ = {/ v}
↕ = {^ v} {^ | v}

crossed arrow
↚ = {← /} {<- /} </ </-
↛ = {→ /} {-> /} /> -/>

2-headed arrow
↠ = ->> >> →> →→
↞ = <<- << <← ←←

arrow with tail
↢ = ←< <-< <<
↣ = >→ >-> >>

arrow with bar
↤ = ←| <-| <|
↦ = |→ |-> |>

double arrow
⇐ = <= {← =}
⇒ = => {→ =}
⇑ = {= ^} {| | ^}
⇓ = {= v} {| | v}
⇔ = <=> ⇐> <⇒ {↔ =}
