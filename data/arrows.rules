# horisontal arrow
← = 1: <- <-- <
    X: -<
→ = 1: -> --> >
    X: >-
↔ = X: {`←` `→`}
    3: <>
    2: ←→ <-->
    1: <->

# vertical arrow
↑ = 1: ^ {| ^}
↓ = 1: v V {| (v V)}
↕ = X: {`↑` `↓`}
    2: {(↑ ^| |^) (↓ v| |v)}
    1: {^ v} v|^ ^|v

# diagonal arrow
↖ = X: {\ `←`}
    2: (^ ← <- <)\
    1: \(^ ← <- <)
    1: ^-
↗ = X: {/ `→`}
    2: (^ → -> >)/
    1: /(^ → -> >)
    1: -^
↙ = X: {/ `←`}
    2: /(v V ← <- <)
    1: (v V ← <- <)/
    1: v- V-
↘ = X: {\ `→`}
    2: (v V → -> >)\
    1: \(v V → -> >)
    1: -v -V

# double arrow
⇐ = X: {`←` =}
    3: =<
    1: <= ←=
⇒ = X: {`→` =}
    3: >=
    1: => =→
⇔ = X: `⇐``⇒` {`↔` =}
    2: <==>
    1: <=> ⇐⇒ ⇐> <⇒ {↔ =}
⇑ = X: {(`⇒` `⇐`) ^}
    1: {(= ⇒ =>) ^}
    1: {| | ^}
⇓ = X: {(`⇒` `⇐`) (v V)}
    1: {(= ⇐ <=) (v V)}
    1: {| | (v V)}

# crossed arrow
↚ = X: {`←` /}
    2: {/ (<- ← <)}
    1: </- <-/-
↛ = X: {`→` /}
    2: {/ (-> → >)}
    1: -/> -/->
⇍ = X: {`⇐` /}
    2: {/ (<= ⇐)}
    1: </= <=/= <≠
⇏ = X: {`⇒` /}
    2: {/ (=> ⇒)}
    1: =/> =/=> ≠>

# 2-headed arrow
↠ = X: `→`>
    3: →→
    4: >>
    3: »(- →)
    1: (- →)»
    3: >(-> →)
    1: (-> →)>
↞ = X: <`←`
    3: ←←
    4: <<
    3: (- ←)«
    1: «(- ←)
    3: (<- ←)<
    1: <(<- ←)
↟ = X: `↑``↑`
    2: ↑↑ {|^ ^|} ^^
    1: {^ | ^}
↡ = X: `↓``↓`
    2: ↓↓ {(|v |V) (v| V|)} vv VV
    1: {v | v} {V | V}

# arrow with tail
↢ = X: {`←` <}
    5: <<
    3: <← <<-
    1: ←< <-<
↣ = X: {> `→`}
    5: >>
    3: →> ->>
    1: >→ >->

# arrow with bar
↤ = X: {`←` |}
    2: |<- |<
    1: <-| <|
    1: {← |}
↦ = X: {| `→`}
    2: ->| >|
    1: |-> |>
    1: {→ |}
↥ = X: {`↑` _}
    1: {(↑ ^ |^ ^|) _}
↧ = X: {`↓` _}
    1: {(↓ v V |v |V V| v|) _}
    2: {(t T) (v V)}
    1: TV

# paired arrows
⇄ = X: {`→` `←`}
    3: ←→ <-->
    3: ><
    1: →← -><-
⇆ = X: {`←` `→`}
    3: →← -><-
    3: <>
    1: ←→ <-->
⇉ = X: `→``→`
    3: >> =>
    2: ->> -->
    1: →→ ->-> =» =>> -->> --»
⇇ = X: `←``←`
    3: << <=
    2: <<- <--
    1: ←← <-<- «= <<= <<-- «--
⇅ = X: {`↑` `↓`}
    1: ↑↓ |^v| ^||v
⇵ = X: {`↓` `↑`}
    1: ↓↑ |v^| v||^
⇈ = X: `↑``↑`
    1: ↑↑ ^^
    1: {^ ^ | |}
⇊ = X: `↓``↓`
    1: ↓↓ vv VV
    1: {v v | |} {V V | |}
