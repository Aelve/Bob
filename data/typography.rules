# ellipsis
… = 1: ...

# asterism
⁂ = 1: ***

# interrobang
‽ = 1: {? !}

# en dash
– = 1: - --

# em dash
— = 2: -
    1: ---

# figure dash
‒ = 5: -

# horizontal bar
― = 5: -
    3: ---
    1: ----

# swung dash
⁓ = 1: {`—` ~}
    2: ~~

# apostrophe
’ = 1: ''''

# left single quotation mark
‘ = 1: '`'
    3: ''''

# right single quotation mark
’ = 3: ''''

# left double quotation mark
“ = 2: '"' '``'
    3: ''''''

# right double quotation mark
” = 2: '"'
    3: ''''''

# left guillemet
« = 1: <<
    4: '"'

# right quillemet
» = 1: >>
    4: '"'

# single quotation marks
‘’ = 1: ''''''

# double quotation marks
“” = 1: '""'

# guillemets
«» = 2: '""'

# service mark
℠ = 1: sm SM

# trademark
™ = 1: tm TM

# copyright
© = 1: '(c)' '(C)' {(o O) (c C)} c C

# sound-recording copyright
℗ = 1: '(p)' '(P)' {(o O) (p P)} p P

# registered trademark
® = 1: '(r)' '(R)' {(o O) (r R)} r R
